#!/bin/bash
echo ""
bin/gn gen out/ios-release --args='is_debug=false is_official_build=false target_os="ios" target_cpu="arm64" ios_min_target = "13.0" skia_ios_identity=".*Bussaya Tejasen.*" skia_ios_profile="/Users/jimmy/keys/JimmyDev2022.mobileprovision"' --ide=xcode
echo ""
echo "Build cmd:"
echo "ninja -C out/ios-release HelloWorld"
echo "ninja -C out/ios-release HelloWorldApp"
echo "ninja -C out/ios-release SkottieViewerApp"
echo "ninja -C out/ios-release viewer"
echo ""
echo "Deploy to device:"
echo "b/ios-release-deploy HelloWorld.app"
echo "b/ios-release-deploy HelloWorldApp.app"
echo "b/ios-release-deploy SkottieViewerApp.app"
echo "b/ios-release-deploy viewer.app"
echo ""
